{
  "name": "resume",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "ci:check": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "pre-commit": "npm run ci:check",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "fresh-install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "bundle-analysis": "npm run build && du -sh dist && find dist -name '*.js' -exec du -h {} \\; | sort -hr | head -10",
    "security:check": "npm audit --audit-level=moderate && npm run outdated:check",
    "outdated:check": "npm outdated",
    "health:check": "npm run type-check && npm run lint && npm run security:check && npm run build",
    "deploy:dry-run": "npm run health:check && npm run preview"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/react": "^4.3.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.5.0",
    "@tailwindcss/typography": "^0.5.16",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2"
  }
}
